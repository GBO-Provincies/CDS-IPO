    <Primitive path="app:status" mapping="status" />
    <Primitive path="app:diercategorie" mapping="diercategorie" />
    <Primitive path="app:rav_code" mapping="rav_code" />
    <Primitive path="app:dieraantal" mapping="dieraantal" />
    <Primitive path="app:nh3_factor" mapping="nh3_factor" />
    <Primitive path="app:gebouwhoogte" mapping="gebouwhoogte" />
